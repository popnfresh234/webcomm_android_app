{
  "name": "com.webcomm.fido.vue",
  "displayName": "FidoUafApp",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "run-android": "export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64 && export PATH=$JAVA_HOME/bin:$PATH && cordova run android",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-add-swift-support": "^2.0.2",
    "cordova-plugin-authsdk": "file:res/customPlugins/cordova-plugin-authsdk",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-qrscanner": "^3.0.1"
  },
  "devDependencies": {
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-whitelist": "^1.3.5"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-authsdk": {
        "AUTH_PATTERN_FOOTER_BUTTON_TEXT_COLOR": "#F0F0F0",
        "AUTH_PATTERN_FOOTER_BACKGROUND_COLOR": "#38435A",
        "AUTH_PATTERN_LINE_INVALID_COLOR": "#d70c18",
        "AUTH_PATTERN_DOTS_INVALID_COLOR": "#d70c18",
        "AUTH_PATTERN_LINE_COLOR": "#888888",
        "AUTH_PATTERN_DOTS_COLOR": "#888888",
        "AUTH_PATTERN_BACKGROUND_COLOR": "#F0F0F0",
        "AUTH_ERROR_TEXT_COLOR": "#d70c18",
        "AUTH_INFO_TEXT_COLOR": "#888888",
        "AUTH_PATTERN_LOCKOUT_POLICY_FLAG": "2-3|1-3-30|1-3-60|3-lock",
        "REG_PATTERN_FOOTER_BUTTON_TEXT_COLOR": "#F0F0F0",
        "REG_PATTERN_FOOTER_BACKGROUND_COLOR": "#38435A",
        "REG_PATTERN_LINE_INVALID_COLOR": "#d70c18",
        "REG_PATTERN_DOTS_INVALID_COLOR": "#d70c18",
        "REG_PATTERN_LINE_COLOR": "#888888",
        "REG_PATTERN_DOTS_COLOR": "#888888",
        "REG_PATTERN_BACKGROUND_COLOR": "#F0F0F0",
        "REG_ERROR_TEXT_COLOR": "#d70c18",
        "REG_INFO_TEXT_COLOR": "#888888",
        "REG_HEADER_TITLE_COLOR": "#F0F0F0",
        "REG_HEADER_BACKGROUND_COLOR": "#38435A",
        "REG_REUSED_FLAG": "N",
        "BIOMETRIC_SECURITY_LEVEL": "Strong",
        "BIOMETRIC_TIER": "Strong",
        "PATTERN_LOCK_ACTIVE": "Y",
        "BIOMETRIC_ACTIVE": "Y",
        "TRANSLATION_PATH": "plugins/cordova-plugin-authsdk/translations/app/"
      },
      "cordova-plugin-qrscanner": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-file": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}